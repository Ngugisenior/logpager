{
  "name": "logpager",
  "displayName": "logpager",
  "description": "An extension to open large files by paging",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.95.0"
  },
  "repository": "",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "main": "./dist/extension.js",
  "contributes": {},
  "scripts": {
    "set-publisher": "node scripts/setPublisher.js",
    "vscode:prepublish": "npm run set-publisher && npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "npm run set-publisher && webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "publisher": "JosephMuiruri"
}